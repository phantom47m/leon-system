{
  "name": "openclaw-voice-gpt-realtime",
  "version": "0.1.2",
  "description": "Ultra-low-latency AI phone calls powered by OpenAI's Realtime API for OpenClaw",
  "module": "index.ts",
  "main": "index.ts",
  "type": "module",
  "keywords": [
    "openclaw",
    "voice",
    "phone",
    "call",
    "realtime",
    "openai",
    "twilio",
    "ai-calling",
    "speech-to-speech"
  ],
  "author": "Connor Callison",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/connorcallison/openclaw-voice-gpt-realtime.git"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ]
  },
  "scripts": {
    "dev": "bun run --watch index.ts",
    "start": "bun run index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sinclair/typebox": "0.34.48",
    "twilio": "^5.4.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/ws": "^8.5.13"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
